{
  "name": "culture-logistic",
  "version": "1.0.0",
  "main": "index.js",
  "private": true,
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack serve --config webpack.config.dev.js",
    "build_dev": "cross-env NODE_ENV=production webpack --env SERVER=dev --config webpack.config.prod.js",
    "build_prod": "cross-env NODE_ENV=production webpack --env SERVER=prod --config webpack.config.prod.js",
    "lint": "eslint ./src --fix --color",
    "deploy": "node scripts/deploy.js",
    "test": "npm-run-all --parallel lint checktypes",
    "checktypes": "tsc --noEmit --extendedDiagnostics"
  },
  "husky": {
    "hooks": {
      "prepare-commit-msg": "echo 12 & exec < /dev/tty && git cz --hook || true",
      "pre-commit": "lint-staged",
      "post-checkout": "node node_modules/@consta/widgets-configs/scripts/checkout-clean.js"
    }
  },
  "lint-staged": {
    "{src,types}/**/*.{ts,tsx}": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ],
    "{src}/**/*.css": [
      "prettier --write",
      "stylelint --fix",
      "git add"
    ],
    "{src,scripts,config}/**/*.{json,js}": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ]
  },
  "publishConfig": {
    "access": "public"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": "./commitizen.js"
    }
  },
  "dependencies": {
    "@bem-react/classname": "^1.5.11",
    "@consta/charts": "^0.5.0",
    "@consta/uikit": "^3.0.0",
    "@react-google-maps/api": "^2.7.0",
    "@reduxjs/toolkit": "^1.6.1",
    "@trendyol-js/react-carousel": "^2.0.0",
    "@types/google-map-react": "^2.1.3",
    "@types/google.maps": "^3.47.2",
    "axios": "0.21.1",
    "classnames": "^2.3.1",
    "cookies-js": "^1.2.3",
    "emoji-picker-react": "^3.4.8",
    "file-saver": "^2.0.5",
    "js-cookie": "^2.2.1",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "optional-failure": "^1.0.0",
    "query-string": "^7.0.1",
    "react": "^17.0.2",
    "react-big-calendar": "^0.40.1",
    "react-dom": "^17.0.2",
    "react-hot-toast": "^2.1.1",
    "react-redux": "^7.2.4",
    "react-router-dom": "^5.2.0",
    "react-scroll": "^1.8.4",
    "react-text-mask": "^5.4.3",
    "react-top-loader": "^1.0.0",
    "react-transition-group": "^4.4.2",
    "redux": "^4.1.1",
    "redux-persist": "^6.0.0",
    "redux-persist-cookie-storage": "^1.0.0",
    "redux-thunk": "^2.3.0",
    "yup": "^0.32.9"
  },
  "devDependencies": {
    "@babel/core": "^7.14.8",
    "@babel/plugin-transform-runtime": "^7.14.5",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.14.8",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.14.5",
    "@babel/runtime": "^7.14.8",
    "@consta/widgets-configs": "1.5.0",
    "@consta/widgets-utils": "0.2.14",
    "@svgr/webpack": "^5.5.0",
    "@types/file-saver": "^2.0.3",
    "@types/js-cookie": "^2.2.7",
    "@types/moment": "^2.13.0",
    "@types/node": "^16.4.0",
    "@types/react": "^17.0.14",
    "@types/react-big-calendar": "^0.38.0",
    "@types/react-dom": "^17.0.9",
    "@types/react-redux": "^7.1.18",
    "@types/react-router-dom": "^5.1.8",
    "@types/react-scroll": "^1.8.3",
    "@types/redux-logger": "^3.0.9",
    "@types/uuid": "^8.3.1",
    "@types/webpack": "^5.28.0",
    "@types/webpack-dev-server": "^3.11.5",
    "@typescript-eslint/eslint-plugin": "^4.28.4",
    "@typescript-eslint/parser": "^4.28.4",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^4.0.0-alpha.0",
    "commitizen": "^4.2.3",
    "copy-webpack-plugin": "^9.0.1",
    "cross-env": "^7.0.3",
    "css-loader": "^6.2.0",
    "css-minimizer-webpack-plugin": "^3.0.2",
    "cz-customizable": "^6.3.0",
    "dotenv-webpack": "^7.0.3",
    "eslint": "^7.31.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-todo-plz": "1.1.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.3.2",
    "husky": "4.3.8",
    "json-loader": "^0.5.7",
    "lint-staged": "11.1.2",
    "npm-run-all": "4.1.5",
    "prettier": "^2.3.2",
    "redux-logger": "^3.0.6",
    "sass": "^1.35.2",
    "sass-loader": "^12.1.0",
    "source-map-loader": "^3.0.0",
    "ssh2-sftp-client": "^7.0.3",
    "style-loader": "^3.2.1",
    "svg-url-loader": "^7.1.1",
    "terser-webpack-plugin": "^5.1.4",
    "tsconfig-paths-webpack-plugin": "^3.5.1",
    "typescript": "^4.3.5",
    "webpack": "^5.45.1",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^3.11.2"
  }
}
